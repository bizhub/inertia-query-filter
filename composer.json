{
    "name": "bizhub/inertia-query-filter",
    "description": "Laravel + Inertiajs query filters",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/bizhub/inertia-query-filter",
    "authors": [
        {
            "name": "Lex van der Woude",
            "email": "lex@bizhub.co.nz",
            "role": "Developer"
        }
    ],
    "require": {
        "inertiajs/inertia-laravel": "^0.6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Bizhub\\QueryFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bizhub\\QueryFilter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bizhub\\QueryFilter\\QueryFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
